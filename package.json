{
  "name": "loyverse",
  "version": "0.1.1",
  "description": "Modern Node.js client for the Loyverse API",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bob6664569/loyverse.git"
  },
  "bugs": {
    "url": "https://github.com/bob6664569/loyverse/issues"
  },
  "homepage": "https://github.com/bob6664569/loyverse#readme",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [
    "loyverse",
    "api",
    "pos",
    "client",
    "retail"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "engines": {
    "node": ">=16"
  },
  "author": "bob6664569",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "8.22.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  }
}
